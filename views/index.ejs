<!DOCTYPE html>
<html>
  <head>
    <title>CORONA DASHBOARD </title>

    <h1 style="text-align: center;">WORLD AND INDIA CORONA DASHBOARD</h1>
    
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <!-- <h1><%= india %></h1> -->


    <br><br><br><br><br>

    <div style="text-align:center;"></div>
          <table>
            <caption> <h1> INDIAN STATE RECORDS </h1> </caption>
              <tr>
                <th>State</th>
                <th>active</th>
                <th>confirmed</th>
                <th>deaths</th>
                <th>recovered</th>
              </tr>
        
            <% for (var i = 0; i < india.length; i++) { %>
              <tr>
                <td><%= india[i].state %></td>
                <td><%= india[i].active %></td>
                <td><%= india[i].confirmed %></td>
                <td><%= india[i].deaths %></td>
                <td><%= india[i].recovered %></td>
              </tr>    
            <% } %>
        </table>
  </div>


  
<br><br><br><br><br><br><br><br>

<div style="text-align:center;">
      <table>
        <caption> <h1>WORLD COUNTRY RECORDS </h1></h1></caption>
        <tr>
          <th>Country</th>
          <th>TotalConfirmed</th>
          <th>TotalDeaths</th>
          <th>TotalRecovered</th>
        </tr>
    
        <% for (var i = 0; i < world.length; i++) { %>
        <tr>
          <td><%= world[i].Country %></td>
          <td><%= world[i].TotalConfirmed %></td>
          <td><%= world[i].TotalDeaths %></td>
          <td><%= world[i].TotalRecovered %></td>
        </tr>    
        <% } %>
    </table>
</div>
   
   
   




  </body>
</html>
